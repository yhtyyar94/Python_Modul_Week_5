class Rectangle:
    def __init__(self, width, height):
        self.width = width
        self.height = height

    def area(self):
        return self.width * self.height

    def perimeter(self):
        return 2 * (self.width + self.height)

# Dikdörtgen örneği
rect = Rectangle(5, 7)
print("Dikdörtgenin Alanı:", rect.area())
print("Dikdörtgenin Çevresi:", rect.perimeter())
